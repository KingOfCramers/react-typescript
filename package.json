{
  "name": "ts_cloture.app.frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.1.0",
    "@apollo/react-hooks": "^4.0.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "node-sass": "^4.14.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "^3.1.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.0",
    "typescript": "^3.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "scripts": {
    "start": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --verbose --watchAll --env=jsdom-fourteen",
    "test:coverage": "SKIP_PREFLIGHT_CHECK=true react-scripts test --coverage --watchAll=false --env=jsdom-fourteen",
    "test:view": "live-server coverage/lcov-report",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.22.2",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "jest": "^26.4.0",
    "jest-fetch-mock": "^3.0.3",
    "live-server": "^1.2.1",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.2.0"
  }
}
